{
  "name": "mzr",
  "version": "1.0.0",
  "description": "MZR is an extension that helps you see the breakpoints in your text, so you can create the measure that you want. This is meant to be a tool for web typography nerds.",
  "main": "src/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "parcel build src/index.js -d dist/ -o index.js",
    "dev": "parcel src/index.html --open 'google chrome canary'",
    "icons": "cp ./src/icons/* ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "parcel-bundler": "^1.11.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {}
}
